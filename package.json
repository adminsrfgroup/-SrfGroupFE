  "scripts": {
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "start:dev": "env-cmd -f ./environments/.dev.env react-scripts start",
    "build": "env-cmd -f ./environments/.prod.env npm run react:scripts:build",
    "react:scripts:build-without-source-map": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "react:scripts:build": "react-scripts build",
    "analyze:bundle": "source-map-explorer 'build/static/js/*.js'",
    "test-with-watch": "react-scripts test --watchAll=true",
    "test": "react-scripts test --watchAll=false",
    "coverage": "react-scripts test --coverage",
    "test-sonar": "react-scripts test --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter",
    "eject": "react-scripts eject",
    "e2e-cypress": "npx cypress open",
    "serve:build:prod": "npm run build:prod && serve -s build",
    "generate-sonar-rapport": "node sonar-project.js",
    "docker:sonar:up": "docker-compose -f docker/sonar.yml up -d",
    "docker:sonar:down": "docker-compose -f docker/sonar.yml down -v --remove-orphans",
    "docker:react:up": "docker-compose -f react.yml up ",
    "docker:react:down": "docker-compose -f react.yml down -v --remove-orphans",
    "clear-cache-jest": "jest --clearCache",
    "format-line-prettier": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint:prettier:project": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/addon-onboarding": "^1.0.8",
